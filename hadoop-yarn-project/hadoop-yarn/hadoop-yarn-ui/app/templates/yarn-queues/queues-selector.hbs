<div class="container-fluid">
  <div class="row">
    <div class="col-lg-4">
      <div class="input-group">
        <input type="text" class="form-control" placeholder="Enter Queue Name...">
        <span class="input-group-btn">
          <button class="btn btn-default" type="button">Go To Queue!</button>
        </span>
      </div><!-- /input-group -->
    </div><!-- /.col-lg-6 -->
  </div><!-- /.row -->

  <!-- queue selector -->
  <div class="row">
    <div class="col-md-12 container-fluid" id="tree-selector-container">
       {{tree-selector model=model parentId="tree-selector-container"}}
    </div>
  </div>
</div>

{{outlet}}